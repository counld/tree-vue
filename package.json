{
  "name": "tree-test",
  "version": "0.1.0",
  "description": "这是一个node服务搭建的vue项目,采用mysql数据库,",
  "private": true,
  "keywords": ["express", "mysql", "vue2", "vue-axios"],
  "license": "ISC",
  "scripts": {
    "start": "node ./service/index.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.9",
    "express": "^4.18.1",
    "log4js": "^6.6.1",
    "mysql": "^2.18.1",
    "vue": "^2.6.11",
    "vue-axios": "^3.4.1",
    "vue-router": "^3.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "no-mixed-spaces-and-tabs": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
